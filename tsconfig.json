{
  "compilerOptions": {
    "target": "ES2022",
    "baseUrl": "src",
    "paths": {
      "@root/*": ["*"],
      "@routes/*": ["routes/*"],
      "@sql/*": ["sql/*"],
      "@sf/*": ["sf/*"],
      "@cube/*": ["cube/*"],
      "@type/*": ["types/*"],
      "@core/*": ["core/*"],
      "@k6/*": ["k6/*"]
    },
    "types": [
      "node"
    ],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strictPropertyInitialization": false,
    "moduleResolution": "node"
  }
}
